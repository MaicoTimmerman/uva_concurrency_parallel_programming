set datafile commentschars "#!%"

set boxwidth 0.5
set style line 2
set style fill solid border -1

set grid y
set yrange [0:*]

set title "Average sentiment of most used #hashtags"
set xlabel "Hashtag"
set ylabel "Average sentiment, with standard deviation"
set xtics rotate

set terminal png size 1600,1000
set output "sentiment.png"

plot 'data_speedup.dat' using 1:3:xtic(2) with boxes title "Average sentiment" linecolor rgb "#0000FF"
